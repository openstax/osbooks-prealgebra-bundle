<problem id="fs-id2497082">
<para id="fs-id2497084"><m:math><m:mrow><m:mn>0.96</m:mn><m:mo>÷</m:mo><m:mo stretchy="false">(</m:mo><m:mn>−12</m:mn><m:mo stretchy="false">)</m:mo></m:mrow></m:math></para>
</problem>
