</problem>
</exercise>
<exercise id="fs-id2412355">
<problem id="fs-id2862126">
<para id="fs-id2862128"><m:math><m:mrow><m:mi>h</m:mi><m:mo>+</m:mo><m:mn>5.91</m:mn><m:mo>=</m:mo><m:mn>2.4</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id2862144">
<para id="fs-id2862146"><emphasis effect="italics">h</emphasis> = −3.51</para>
</solution>
</exercise>
<exercise id="fs-id2338343">
<problem id="fs-id2338345">
<para id="fs-id2338347"><m:math><m:mrow><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>2.26</m:mn><m:mo>=</m:mo><m:mn>−1.1</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id2338364">
<problem id="fs-id2338366">
