</problem>
</exercise>
<exercise id="fs-id2444839">
<problem id="fs-id2444842">
<para id="fs-id2444844"><m:math><m:mrow><m:mn>24</m:mn><m:mo>÷</m:mo><m:mo stretchy="false">(</m:mo><m:mn>0.1</m:mn><m:mo>+</m:mo><m:mn>0.02</m:mn><m:mo stretchy="false">)</m:mo></m:mrow></m:math></para>
</problem>
<solution id="fs-id1751776">
