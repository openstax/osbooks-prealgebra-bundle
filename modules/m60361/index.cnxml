    <entry/>
    <entry><media id="eip-id1167835483621" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_011i_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry/>
    <entry><media id="eip-id1167835483637" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_011j_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry/>
    <entry><media id="eip-id1167835483653" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_011k_img_new.jpg"/></media></entry>
