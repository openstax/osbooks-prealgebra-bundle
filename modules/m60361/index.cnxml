    <entry><media id="eip-id1167835368312" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_006h_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry>Solve.</entry>
    <entry><media id="eip-id1167835368330" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_006i_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry/>
    <entry><media id="eip-id1167835368346" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_08_06_006j_img_new.jpg"/></media></entry>
