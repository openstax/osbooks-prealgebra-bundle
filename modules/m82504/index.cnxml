<entry valign="middle" align="center">Cost</entry>
</row>
</thead>
<tbody>
<row valign="top">
<entry align="center" valign="top">1</entry>
<entry align="center" valign="top"><m:math><m:mrow><m:mtext>$</m:mtext><m:mn>20,000</m:mn><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>0.90</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
</row>
<row valign="top">
<entry align="center" valign="top">2</entry>
<entry align="center" valign="top"><m:math><m:mrow><m:mtext>$</m:mtext><m:mn>20,000</m:mn><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>0.90</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup></m:mrow></m:math></entry>
</row>
