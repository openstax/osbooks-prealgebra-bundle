    <entry namest="c1" nameend="c2"/>
    <entry><media id="eip-id1169748502596" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_008d_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry namest="c1" nameend="c2">Multiply by the reciprocal of <m:math><m:mo>−</m:mo><m:mfrac><m:mn>4</m:mn><m:mn>5</m:mn></m:mfrac></m:math>.</entry>
    <entry><media id="eip-id1169754228835" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_008e_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry namest="c1" nameend="c2">Reciprocals multiply to 1.</entry>
    <entry><media id="eip-id1169754228856" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_008f_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry namest="c1" nameend="c2">Multiply.</entry>
    <entry><media id="eip-id1169754228876" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_008g_img_new.jpg"/></media></entry>
  </row>
  <row>
    <entry>Check:</entry>
    <entry><media id="eip-id1169752143124" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_008a_img_new.jpg"/></media></entry>
