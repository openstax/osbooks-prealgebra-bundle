    <entry>Check:</entry>
    <entry><media id="eip-id1169751061744" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_010a_img_new.jpg"/></media></entry>
    <entry/>
  </row>
  <row>
    <entry>Substitute <m:math><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>−5</m:mn></m:math>.</entry>
    <entry><media id="eip-id1169753281414" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_010b_img_new.jpg"/></media></entry>
    <entry/>
  </row>
  <row>
    <entry/>
    <entry><media id="eip-id1169747517349" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_010c_img_new.jpg"/></media></entry>
    <entry/>
  </row>
  <row>
    <entry/>
    <entry><media id="eip-id1169747517368" alt="."><image mime-type="image/png" src="../../media/CNX_ElemAlg_Figure_02_02_010d_img_new.jpg"/></media></entry>
