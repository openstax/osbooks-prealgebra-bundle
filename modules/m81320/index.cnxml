</solution>
</exercise>
<exercise id="fs-id1692464">
<problem id="fs-id1166497163737">
<para id="fs-id1273359"><m:math><m:mrow><m:mn>3</m:mn><m:mrow><m:mo stretchy="false">(</m:mo><m:mrow><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>5</m:mn></m:mrow><m:mo stretchy="false">)</m:mo></m:mrow><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>−7</m:mn></m:mrow></m:math></para>
