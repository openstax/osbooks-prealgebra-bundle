</solution>
</exercise>
<exercise id="fs-id1166490838761">
<problem id="fs-id1704419">
<para id="fs-id1166483686090"><m:math><m:mrow><m:mi>p</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>2</m:mn><m:mn>5</m:mn></m:mfrac><m:mo>=</m:mo><m:mfrac><m:mn>2</m:mn><m:mn>3</m:mn></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1388316">
<problem id="fs-id1736339">
<para id="fs-id1166497423982"><m:math><m:mrow><m:mi>y</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac><m:mo>=</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>5</m:mn></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1166497245629">
