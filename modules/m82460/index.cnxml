</solution>
</exercise>
<exercise id="fs-id1170654008310">
<problem id="fs-id1170654008312">
<para id="fs-id1170654008314"><m:math><m:mrow><m:mn>18</m:mn><m:mo>−</m:mo><m:mn>4</m:mn><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1170654008356">
<problem id="fs-id1170654008358">
<para id="fs-id1170654008360"><m:math><m:mrow><m:mn>4</m:mn><m:mo>−</m:mo><m:mn>11</m:mn><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>3</m:mn><m:mi>c</m:mi><m:mo>−</m:mo><m:mn>2</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></para>
</problem>
<solution id="fs-id1170654008387">
<para id="fs-id1170654008389"><m:math><m:mrow><m:mn>−33</m:mn><m:mi>c</m:mi><m:mo>+</m:mo><m:mn>26</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1170654008404">
<problem id="fs-id1170654008406">
<para id="fs-id1170654008408"><m:math><m:mrow><m:mn>9</m:mn><m:mo>−</m:mo><m:mn>6</m:mn><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>7</m:mn><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>5</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></para>
</problem>
</exercise>
