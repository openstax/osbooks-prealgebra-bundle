</row>
<row valign="top">
<entry valign="top" align="center"><emphasis effect="bold"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mi>n</m:mi></m:msup></m:mrow></m:math></emphasis></entry>
<entry valign="middle" align="center"><m:math><m:mtable><m:mtr><m:mtd columnalign="center"><m:msup><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>−1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>1</m:mn></m:msup></m:mtd></m:mtr><m:mtr/><m:mtr><m:mtd columnalign="center"><m:mn>−1</m:mn></m:mtd></m:mtr></m:mtable></m:math></entry>
<entry valign="middle" align="center"><m:math><m:mtable><m:mtr><m:mtd columnalign="center"><m:msup><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>−1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>2</m:mn></m:msup></m:mtd></m:mtr><m:mtr/><m:mtr><m:mtd columnalign="center"><m:mn>1</m:mn></m:mtd></m:mtr></m:mtable></m:math>
</entry>
<entry valign="middle" align="center"><m:math><m:mtable><m:mtr><m:mtd columnalign="center"><m:msup><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>−1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>3</m:mn></m:msup></m:mtd></m:mtr><m:mtr/><m:mtr><m:mtd columnalign="center"><m:mn>−1</m:mn></m:mtd></m:mtr></m:mtable></m:math>
</entry>
<entry valign="middle" align="center"><m:math><m:mtable><m:mtr><m:mtd columnalign="center"><m:msup><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>−1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>4</m:mn></m:msup></m:mtd></m:mtr><m:mtr/><m:mtr><m:mtd columnalign="center"><m:mn>1</m:mn></m:mtd></m:mtr></m:mtable></m:math>
</entry>
<entry valign="middle" align="center"><m:math><m:mtable><m:mtr><m:mtd columnalign="center"><m:msup><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>−1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>5</m:mn></m:msup></m:mtd></m:mtr><m:mtr/><m:mtr><m:mtd columnalign="center"><m:mn>−1</m:mn></m:mtd></m:mtr></m:mtable></m:math>
</entry>
</row>
