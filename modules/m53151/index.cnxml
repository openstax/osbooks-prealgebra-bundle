 <entry><newline/><m:math><m:mi>V</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>3</m:mn></m:mfrac><m:mspace width="1em"/><m:mi>π</m:mi><m:mspace width="2.3em"/><m:msup><m:mi>r</m:mi><m:mn>2</m:mn></m:msup><m:mspace width="2em"/><m:mi>h</m:mi></m:math><newline/><m:math><m:mi>V</m:mi><m:mo>≈</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>3</m:mn></m:mfrac><m:mspace width="0.7em"/><m:mn>3.14</m:mn><m:mspace width="1em"/><m:msup><m:mrow><m:mo stretchy="false">(</m:mo><m:mn>2.5</m:mn><m:mo stretchy="false">)</m:mo></m:mrow><m:mn>2</m:mn></m:msup><m:mspace width="1em"/><m:mo stretchy="false">(</m:mo><m:mn>8</m:mn><m:mo stretchy="false">)</m:mo></m:math></entry>
