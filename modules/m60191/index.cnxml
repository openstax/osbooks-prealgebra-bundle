</problem>
<solution id="fs-id1168344306447">
<para id="fs-id1168344306449"><m:math><m:mrow><m:mo stretchy="false">(</m:mo><m:mi>w</m:mi><m:mo>−</m:mo><m:mn>9</m:mn><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mi>w</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo></m:mrow></m:math></para>
</solution>
</exercise>
