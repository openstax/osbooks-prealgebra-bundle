<solution id="fs-id1168343054835">
<para id="fs-id1168344318324">prime</para>
</solution>
</exercise>
<exercise id="fs-id1168344318329">
<problem id="fs-id1168344318331">
<para id="fs-id1168344318334"><m:math><m:mrow><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>9</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1168344192265">
<problem id="fs-id1168344192268">
<para id="fs-id1168344192270"><m:math><m:mrow><m:mn>8</m:mn><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:math></para>
