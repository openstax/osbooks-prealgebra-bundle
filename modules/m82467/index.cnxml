</solution>
</exercise>
<exercise id="fs-id1166503439230">
<problem id="fs-id1166503439232">
<para id="fs-id1166503439234"><m:math><m:mrow><m:mn>2</m:mn><m:mo stretchy="false">(</m:mo><m:mn>9</m:mn><m:mi>s</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:mn>62</m:mn><m:mo>=</m:mo><m:mn>16</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1166503194154">
<problem id="fs-id1166503194156">
<para id="fs-id1166503194158"><m:math><m:mrow><m:mn>8</m:mn><m:mo stretchy="false">(</m:mo><m:mn>6</m:mn><m:mi>t</m:mi><m:mo>−</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>−</m:mo><m:mn>35</m:mn><m:mo>=</m:mo><m:mn>−27</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1166503228064">
<para id="fs-id1166503228066"><m:math><m:mrow><m:mi>t</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1166503219738">
<problem id="fs-id1166503219740">
<para id="fs-id1166503219742"><m:math><m:mrow><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>10</m:mn><m:mo>−</m:mo><m:mn>2</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>54</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
