<para id="fs-id1167835331250"><m:math><m:mrow><m:msup><m:mi>v</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>80</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167832059836">
<problem id="fs-id1167835233285">
<para id="fs-id1167834433241"><m:math><m:mrow><m:mn>4</m:mn><m:msup><m:mi>m</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>36</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167831890697">
<para id="fs-id1167834556624"><m:math><m:mrow><m:mi>m</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>3</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834526088">
<problem id="fs-id1167830991315">
<para id="fs-id1167835197358"><m:math><m:mrow><m:mn>3</m:mn><m:msup><m:mi>n</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>48</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834555217">
<problem id="fs-id1167835361967">
<para id="fs-id1167827956966"><m:math><m:mrow><m:mfrac><m:mn>4</m:mn><m:mn>3</m:mn></m:mfrac><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>48</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167832153763">
<para id="fs-id1167835384779"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>6</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835253672">
<problem id="fs-id1167835226292">
<para id="fs-id1167834525115"><m:math><m:mrow><m:mfrac><m:mn>5</m:mn><m:mn>3</m:mn></m:mfrac><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>60</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167831907905">
<problem id="fs-id1167835609305">
<para id="fs-id1167835238557"><m:math><m:mrow><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>25</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834061966">
<para id="fs-id1167831180559"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>5</m:mn><m:mi>i</m:mi></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834431071">
<problem id="fs-id1167835231164">
<para id="fs-id1167832053103"><m:math><m:mrow><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>64</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835181786">
<problem id="fs-id1167835233043">
<para id="fs-id1167834193442"><m:math><m:mrow><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>63</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834084403">
<para id="fs-id1167831882467"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>3</m:mn><m:msqrt><m:mn>7</m:mn></m:msqrt><m:mi>i</m:mi></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835287845">
<problem id="fs-id1167834422861">
<para id="fs-id1167835244108"><m:math><m:mrow><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>45</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835363294">
<problem id="fs-id1167835194750">
<para id="fs-id1167835348573"><m:math><m:mrow><m:mfrac><m:mn>4</m:mn><m:mn>3</m:mn></m:mfrac><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>2</m:mn><m:mo>=</m:mo><m:mn>110</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835369689">
<para id="fs-id1167834221990"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>9</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834555123">
<problem id="fs-id1167835301692">
<para id="fs-id1167826978699"><m:math><m:mrow><m:mfrac><m:mn>2</m:mn><m:mn>3</m:mn></m:mfrac><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>8</m:mn><m:mo>=</m:mo><m:mn>−2</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834193668">
<problem id="fs-id1167835231774">
<para id="fs-id1167827956922"><m:math><m:mrow><m:mfrac><m:mn>2</m:mn><m:mn>5</m:mn></m:mfrac><m:msup><m:mi>a</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mn>11</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167830702673">
<para id="fs-id1167835336263"><m:math><m:mrow><m:mi>a</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>5</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167832056076">
<problem id="fs-id1167835263171">
<para id="fs-id1167835303756"><m:math><m:mrow><m:mfrac><m:mn>3</m:mn><m:mn>2</m:mn></m:mfrac><m:msup><m:mi>b</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>7</m:mn><m:mo>=</m:mo><m:mn>41</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835198333">
<problem id="fs-id1167835300380">
<para id="fs-id1167835357645"><m:math><m:mrow><m:mn>7</m:mn><m:msup><m:mi>p</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>10</m:mn><m:mo>=</m:mo><m:mn>26</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835264408">
<para id="fs-id1167831985782"><m:math><m:mrow><m:mi>p</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mfrac><m:mrow><m:mn>4</m:mn><m:msqrt><m:mn>7</m:mn></m:msqrt></m:mrow><m:mn>7</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835514357">
<problem id="fs-id1167832075492">
<para id="fs-id1167835301399"><m:math><m:mrow><m:mn>2</m:mn><m:msup><m:mi>q</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>30</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834515914">
<problem id="fs-id1167831909571">
<para id="fs-id1167834535018"><m:math><m:mrow><m:mn>5</m:mn><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>7</m:mn><m:mo>=</m:mo><m:mn>25</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835308957">
<para id="fs-id1167834402996"><m:math><m:mrow><m:mi>y</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mfrac><m:mrow><m:mn>4</m:mn><m:msqrt><m:mrow><m:mn>10</m:mn></m:mrow></m:msqrt></m:mrow><m:mn>5</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167826778876">
<problem id="fs-id1167835419133">
<para id="fs-id1167835258525"><m:math><m:mrow><m:mn>3</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>8</m:mn><m:mo>=</m:mo><m:mn>46</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<para id="fs-id1167831913279"><emphasis effect="bold">Solve Quadratic Equations of the Form <emphasis effect="italics">a</emphasis>(<emphasis effect="italics">x</emphasis> − <emphasis effect="italics">h</emphasis>)<sup>2</sup> = <emphasis effect="italics">k</emphasis> Using the Square Root Property</emphasis></para>
<para id="fs-id1167835512866">In the following exercises, solve each equation.</para>
<exercise id="fs-id1167835421703">
<problem id="fs-id1167832081860">
<para id="fs-id1167835288156"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>u</m:mi><m:mo>−</m:mo><m:mn>6</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>64</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834372330">
<para id="fs-id1167835380484"><m:math><m:mrow><m:mi>u</m:mi><m:mo>=</m:mo><m:mn>14</m:mn><m:mo>,</m:mo><m:mi>u</m:mi><m:mo>=</m:mo><m:mn>−2</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834396045">
<problem id="fs-id1167835495775">
<para id="fs-id1167835306924"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>v</m:mi><m:mo>+</m:mo><m:mn>10</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>121</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835534309">
<problem id="fs-id1167835382041">
<para id="fs-id1167831892751"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>6</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>20</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835354789">
<para id="fs-id1167835414865"><m:math><m:mrow><m:mi>m</m:mi><m:mo>=</m:mo><m:mn>6</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>5</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835218062">
<problem id="fs-id1167835287528">
<para id="fs-id1167832151039"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>n</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>32</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835280021">
<problem id="fs-id1167832015825">
<para id="fs-id1167835304508"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>r</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834429066">
<para id="fs-id1167834395249"><m:math><m:mrow><m:mi>r</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mo>±</m:mo><m:mfrac><m:mrow><m:msqrt><m:mn>3</m:mn></m:msqrt></m:mrow><m:mn>2</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835311296">
<problem id="fs-id1167834194654">
<para id="fs-id1167835530258"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>5</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mn>7</m:mn><m:mrow><m:mn>25</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834584354">
<problem id="fs-id1167835348364">
<para id="fs-id1167834120980"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>y</m:mi><m:mo>+</m:mo><m:mfrac><m:mn>2</m:mn><m:mn>3</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mn>8</m:mn><m:mrow><m:mn>81</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835338151">
<para id="fs-id1167835322047"><m:math><m:mrow><m:mi>y</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac><m:mn>2</m:mn><m:mn>3</m:mn></m:mfrac><m:mo>±</m:mo><m:mfrac><m:mrow><m:mn>2</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow><m:mn>9</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834387561">
<problem id="fs-id1167835280288">
<para id="fs-id1167834429374"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>t</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>5</m:mn><m:mn>6</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mrow><m:mn>11</m:mn></m:mrow><m:mrow><m:mn>25</m:mn></m:mrow></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835335930">
<problem id="fs-id1167835362748">
<para id="fs-id1167835257636"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>7</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>55</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167831164053">
<para id="fs-id1167834065930"><m:math><m:mrow><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>7</m:mn><m:mo>±</m:mo><m:mn>5</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834193405">
<problem id="fs-id1167830898727">
<para id="fs-id1167835360350"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>b</m:mi><m:mo>−</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>9</m:mn><m:mo>=</m:mo><m:mn>39</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167831959284">
<problem id="fs-id1167830959724">
<para id="fs-id1167831148750"><m:math><m:mrow><m:mn>4</m:mn><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>27</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835346391">
<para id="fs-id1167834454071"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>−3</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834533115">
<problem id="fs-id1167835309654">
<para id="fs-id1167835210221"><m:math><m:mrow><m:mn>5</m:mn><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>7</m:mn><m:mo>=</m:mo><m:mn>68</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167826807830">
<problem id="fs-id1167835346106">
<para id="fs-id1167831969906"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>5</m:mn><m:mi>c</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>−27</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834196306">
<para id="fs-id1167828436533"><m:math><m:mrow><m:mi>c</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>5</m:mn></m:mfrac><m:mo>±</m:mo><m:mfrac><m:mrow><m:mn>3</m:mn><m:msqrt><m:mn>3</m:mn></m:msqrt></m:mrow><m:mn>5</m:mn></m:mfrac><m:mi>i</m:mi></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835351226">
<problem id="fs-id1167835377917">
<para id="fs-id1167832151868"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>8</m:mn><m:mi>d</m:mi><m:mo>−</m:mo><m:mn>6</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>−24</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834279138">
<problem id="fs-id1167835225792">
<para id="fs-id1167834301266"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>3</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>11</m:mn><m:mo>=</m:mo><m:mn>−17</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835328567">
<para id="fs-id1167826806981"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac><m:mo>±</m:mo><m:mfrac><m:mrow><m:msqrt><m:mn>7</m:mn></m:msqrt></m:mrow><m:mn>2</m:mn></m:mfrac><m:mi>i</m:mi></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835337231">
<problem id="fs-id1167832058516">
<para id="fs-id1167834377165"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>5</m:mn><m:mo>=</m:mo><m:mn>−23</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835361143">
<problem id="fs-id1167835361146">
<para id="fs-id1167834395208"><m:math><m:mrow><m:msup><m:mi>m</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>4</m:mn><m:mi>m</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>8</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835309477">
<para id="fs-id1167832043343"><m:math><m:mrow><m:mi>m</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167831836332">
<problem id="fs-id1167831826351">
<para id="fs-id1167831826353"><m:math><m:mrow><m:msup><m:mi>n</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>8</m:mn><m:mi>n</m:mi><m:mo>+</m:mo><m:mn>16</m:mn><m:mo>=</m:mo><m:mn>27</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167827957936">
<problem id="fs-id1167834191304">
<para id="fs-id1167831086586"><m:math><m:mrow><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>6</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:mo>=</m:mo><m:mn>12</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834535330">
<para id="fs-id1167834535332"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>3</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>3</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835306997">
<problem id="fs-id1167834182612">
<para id="fs-id1167826851119"><m:math><m:mrow><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>12</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>36</m:mn><m:mo>=</m:mo><m:mn>32</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834397430">
<problem id="fs-id1167835595537">
<para id="fs-id1167834387478"><m:math><m:mrow><m:mn>25</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>30</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>9</m:mn><m:mo>=</m:mo><m:mn>36</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167834132168">
<para id="fs-id1167835514464"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>5</m:mn></m:mfrac><m:mo>,</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>9</m:mn><m:mn>5</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835609642">
<problem id="fs-id1167830700250">
<para id="fs-id1167835332640"><m:math><m:mrow><m:mn>9</m:mn><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>12</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>9</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835332458">
<problem id="fs-id1167830698742">
<para id="fs-id1167835356629"><m:math><m:mrow><m:mn>36</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>24</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>81</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167831824616">
<para id="fs-id1167834395049"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mo>−</m:mo><m:mfrac><m:mn>7</m:mn><m:mn>6</m:mn></m:mfrac><m:mo>,</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac><m:mrow><m:mn>11</m:mn></m:mrow><m:mn>6</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835267920">
<problem id="fs-id1167835267922">
<para id="fs-id1167826938219"><m:math><m:mrow><m:mn>64</m:mn><m:msup><m:mi>x</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>144</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>81</m:mn><m:mo>=</m:mo><m:mn>25</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
</section>
<section id="fs-id1167835345754">
<title>Mixed Practice</title>
<para id="fs-id1167828401482">In the following exercises, solve using the Square Root Property.</para>
<exercise id="fs-id1167835375915">
<problem id="fs-id1167835341559">
<para id="fs-id1167835341561"><m:math><m:mrow><m:mn>2</m:mn><m:msup><m:mi>r</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>32</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167831884545">
<para id="fs-id1167835416729"><m:math><m:mrow><m:mi>r</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>4</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834134205">
<problem id="fs-id1167828327067">
<para id="fs-id1167828327069"><m:math><m:mrow><m:mn>4</m:mn><m:msup><m:mi>t</m:mi><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>16</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167832058338">
<problem id="fs-id1167835287967">
<para id="fs-id1167835287969"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>4</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>28</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835534329">
<para id="fs-id1167831922990"><m:math><m:mrow><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>7</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835329258">
<problem id="fs-id1167835515432">
<para id="fs-id1167835515434"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>b</m:mi><m:mo>+</m:mo><m:mn>7</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>8</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834300078">
<problem id="fs-id1167834300080">
<para id="fs-id1167827940661"><m:math><m:mrow><m:mn>9</m:mn><m:msup><m:mi>w</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>24</m:mn><m:mi>w</m:mi><m:mo>+</m:mo><m:mn>16</m:mn><m:mo>=</m:mo><m:mn>1</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835280098">
<para id="fs-id1167834060244"><m:math><m:mrow><m:mi>w</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mi>w</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>5</m:mn><m:mn>3</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167826986972">
<problem id="fs-id1167826986974">
<para id="fs-id1167834463886"><m:math><m:mrow><m:mn>4</m:mn><m:msup><m:mi>z</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>z</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>49</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835380329">
<problem id="fs-id1167835380331">
<para id="fs-id1167835370753"><m:math><m:mrow><m:msup><m:mi>a</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>18</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835334060">
<para id="fs-id1167827967167"><m:math><m:mrow><m:mi>a</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>3</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835343500">
<problem id="fs-id1167835343502">
<para id="fs-id1167834428858"><m:math><m:mrow><m:msup><m:mi>b</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>108</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835373746">
<problem id="fs-id1167835354072">
<para id="fs-id1167835354074"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>p</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>3</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mn>7</m:mn><m:mn>9</m:mn></m:mfrac></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835186401">
<para id="fs-id1167835186403"><m:math><m:mrow><m:mi>p</m:mi><m:mo>=</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>3</m:mn></m:mfrac><m:mo>±</m:mo><m:mfrac><m:mrow><m:msqrt><m:mn>7</m:mn></m:msqrt></m:mrow><m:mn>3</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834190187">
<problem id="fs-id1167835279955">
<para id="fs-id1167835279957"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>q</m:mi><m:mo>−</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>5</m:mn></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834225086">
<problem id="fs-id1167834225088">
<para id="fs-id1167835340850"><m:math><m:mrow><m:msup><m:mi>m</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>12</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835615581">
<para id="fs-id1167834190499"><m:math><m:mrow><m:mi>m</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mi>i</m:mi></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167834552570">
<problem id="fs-id1167834552572">
<para id="fs-id1167835320572"><m:math><m:mrow><m:msup><m:mi>n</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>48</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>.</m:mo></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167832054758">
<problem id="fs-id1167834376963">
<para id="fs-id1167834376965"><m:math><m:mrow><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>14</m:mn><m:mi>u</m:mi><m:mo>+</m:mo><m:mn>49</m:mn><m:mo>=</m:mo><m:mn>72</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835515212">
<para id="fs-id1167834161535"><m:math><m:mrow><m:mi>u</m:mi><m:mo>=</m:mo><m:mn>7</m:mn><m:mo>±</m:mo><m:mn>6</m:mn><m:msqrt><m:mn>2</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835284618">
<problem id="fs-id1167835309057">
<para id="fs-id1167835309059"><m:math><m:mrow><m:msup><m:mi>v</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>18</m:mn><m:mi>v</m:mi><m:mo>+</m:mo><m:mn>81</m:mn><m:mo>=</m:mo><m:mn>50</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167834228388">
<problem id="fs-id1167834228390">
<para id="fs-id1167835370877"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>m</m:mi><m:mo>−</m:mo><m:mn>4</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>3</m:mn><m:mo>=</m:mo><m:mn>15</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167828326435">
<para id="fs-id1167828326437"><m:math><m:mrow><m:mi>m</m:mi><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>±</m:mo><m:mn>2</m:mn><m:msqrt><m:mn>3</m:mn></m:msqrt></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167826967403">
<problem id="fs-id1167834184492">
<para id="fs-id1167834184494"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>7</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>8</m:mn><m:mo>=</m:mo><m:mn>64</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835370290">
<problem id="fs-id1167835370292">
<para id="fs-id1167834489875"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>4</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167835609619">
<para id="fs-id1167835609621"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>−3</m:mn><m:mo>,</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>−7</m:mn></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167835370982">
<problem id="fs-id1167831890558">
<para id="fs-id1167831890560"><m:math><m:mrow><m:msup><m:mrow><m:mrow><m:mo>(</m:mo><m:mrow><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>4</m:mn></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow><m:mn>2</m:mn></m:msup><m:mo>=</m:mo><m:mn>64</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167835306122">
<problem id="fs-id1167835226254">
<para id="fs-id1167835226256"><m:math><m:mrow><m:mn>6</m:mn><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>29</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1167830703495">
<para id="fs-id1167834473209"><m:math><m:mrow><m:mi>c</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:mfrac><m:mrow><m:mn>5</m:mn><m:msqrt><m:mn>6</m:mn></m:msqrt></m:mrow><m:mn>6</m:mn></m:mfrac></m:mrow></m:math></para>
</solution>
</exercise>
<exercise id="fs-id1167826802345">
<problem id="fs-id1167826802347">
<para id="fs-id1167830963539"><m:math><m:mrow><m:mn>2</m:mn><m:msup><m:mi>d</m:mi><m:mn>2</m:mn></m:msup><m:mo>−</m:mo><m:mn>4</m:mn><m:mo>=</m:mo><m:mn>77</m:mn></m:mrow></m:math></para>
</problem>
</exercise>
<exercise id="fs-id1167831895294">
<problem id="fs-id1167831895296">
