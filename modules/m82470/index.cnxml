<exercise id="fs-id1168345634872">
<problem id="fs-id1168345634874">
<para id="fs-id1168345634877"><m:math><m:mrow><m:mn>5</m:mn><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>15</m:mn><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mo>−</m:mo><m:mi>n</m:mi><m:mo stretchy="false">)</m:mo><m:mo>&lt;</m:mo><m:mn>10</m:mn><m:mo stretchy="false">(</m:mo><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>10</m:mn><m:mi>n</m:mi></m:mrow></m:math></para>
