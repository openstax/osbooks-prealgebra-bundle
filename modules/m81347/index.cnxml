<note id="fs-id1492021" class="try">
<exercise id="fs-id1599499">
<problem id="fs-id1526982">
<para id="fs-id1532419">Factor: <m:math><m:mrow><m:mn>4</m:mn><m:msup><m:mi>y</m:mi><m:mn>2</m:mn></m:msup><m:mo>+</m:mo><m:mn>8</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>12</m:mn><m:mo>.</m:mo></m:mrow></m:math></para>
