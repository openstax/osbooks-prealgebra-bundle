    <entry><span class="token">ⓒ</span></entry>
  </row>
  <row>
    <entry><m:math><m:mn>9</m:mn><m:mo>&gt;</m:mo><m:mn>10</m:mn><m:mo>÷</m:mo><m:mn>2</m:mn></m:math></entry>
